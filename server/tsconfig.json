{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "Node",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "dist"
  },
  "include": ["src/**/*"]
}
